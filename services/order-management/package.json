{
  "name": "pazar-order-management",
  "version": "0.1.0",
  "description": "Order management service for Pazar Plus, handling order processing, shipping, and tracking.",
  "main": "src/server.js",
  "scripts": {
    "prestart": "node src/scripts/prestart.js",
    "start": "node src/server.js",
    "dev": "nodemon --inspect src/server.js",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.js'",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "backup": "node src/scripts/backup.js",
    "restore": "node src/scripts/restore.js"
  },
  "keywords": [
    "ecommerce",
    "orders",
    "shipping"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^5.1.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^7.0.3",
    "pdfkit": "^0.13.0",
    "rate-limit-redis": "^4.2.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.1"
  }
}
